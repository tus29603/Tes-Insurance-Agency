{
  "name": "insurance-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "echo 'Frontend build - use backend/build for API'",
    "start": "node server.js",
    "backend:build": "cd backend && npm install",
    "backend:start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install",
    "build:dev": "NODE_ENV=development vite build",
    "build:prod": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "preview:prod": "npm run build:prod && npm run preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "vite build && gh-pages -d dist",
    "deploy:prod": "npm run build:prod && gh-pages -d dist",
    "deploy:preview": "npm run build:dev && gh-pages -d dist",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test:build": "npm run build && npm run preview"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-router-hash-link": "^2.4.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7",
    "sqlite3": "^5.1.6",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "vite": "^7.1.2"
  }
}
